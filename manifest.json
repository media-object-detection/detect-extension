{
    "manifest_version": 3,
    "name": "Media Object Detection",
    "version": "1.0",
    "description": "Detect items in the video and inform them of the sales site",
    "permissions": [
      "activeTab",
      "storage",
      "scripting",
      "downloads"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  